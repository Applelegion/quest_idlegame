{
    "manifest_version": 2,
    "name": "Territory Idle",
    "version": "1.0",
    "background":{
        "scripts":["background.js"],
        "persistent": false
    },
    "browser_action":{
        "default_title":"Territory Idle",
        "default_popup":"popup.html"
    },
    "permissions":[
        "storage",
        "tabs",
        "alarms"
    ]
    
}